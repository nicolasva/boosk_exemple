%h1 New shipping category

%form#new_shipping_category_form
  %label{for: "shipping_category_name"} Name
  %input{id: "shipping_category_name", name: "shipping_category[name]", value: shipping_category.name}
  .actions
    %a.btn{href: "#/shipping_categories"} Back
    %input.btn.btn-primary{type: "submit", value: "Create"}